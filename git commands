git config --global user.name "Ghassan Aljaafri"
git config --global user.email "ghassan.aljaafri@gmail.com"
git config --global core.editor "code"
git config --global --list

git init
git status
git add <file name>
git commit -m "message"
git log


git remote add origin git@github.com:Ghassan-Aljaafri/Learn-Git.git
git remote -v
git push -u origin master